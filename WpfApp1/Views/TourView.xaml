<UserControl
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenTourClient"
        xmlns:ViewModels="clr-namespace:OpenTourClient.ViewModels" x:Class="OpenTourClient.TourView"
        mc:Ignorable="d">
    <Grid>
        <TextBox Text="{Binding TxtTourName}" HorizontalAlignment="Left" Margin="85,99,0,0" VerticalAlignment="Top" Width="660" Height="26"/>
        <TextBlock Text="{Binding TxtDescription}" HorizontalAlignment="Left" Margin="85,125,0,0" VerticalAlignment="Top" Width="660" Height="173"/>
        <!--<TextBox Text="{Binding TxtTags}" HorizontalAlignment="Left" Margin="85,298,0,0" VerticalAlignment="Top" Width="660" Height="15"/>-->
    </Grid>
</UserControl>
